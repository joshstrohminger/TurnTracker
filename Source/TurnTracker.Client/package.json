{
  "name": "turntracker-client",
  "version": "1.17.1",
  "description": "Turn Tracker",
  "scripts": {
    "start": "ng serve --open",
    "start-server": "http-server -p 8080 -c-1 ../TurnTracker.Server/wwwroot --ssl --key ../TurnTracker.Server/localhost.key --cert ../TurnTracker.Server/localhost.crt -o",
    "update-build-date": "node src/environments/setBuildDate.js",
    "build-prod": "ng build --configuration production",
    "update-service-worker": "node src/updateServiceWorker.js",
    "build": "npm run update-build-date && npm run build-prod && npm run update-service-worker",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^15.2.9",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@angular/service-worker": "^15.2.9",
    "core-js": "^3.6.4",
    "encodr": "^1.2.2",
    "luxon": "^3.4.1",
    "rxjs": "^6.6.7",
    "tslib": "^2.0.0",
    "websocket-extensions": "^0.1.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.9",
    "@angular/cli": "^15.2.9",
    "@angular/compiler-cli": "^15.2.9",
    "@angular/language-service": "^15.2.9",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/luxon": "^3.3.1",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "^8.6.2",
    "tslint": "~6.1.0",
    "typescript": "~4.9.5",
    "webpack-subresource-integrity": "^1.5.2"
  }
}

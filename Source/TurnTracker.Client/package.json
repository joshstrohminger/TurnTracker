{
  "name": "turntracker-client",
  "version": "1.15.1",
  "description": "Turn Tracker",
  "scripts": {
    "start": "ng serve --open",
    "start-server": "http-server -p 8080 -c-1 ../TurnTracker.Server/wwwroot --ssl --key ../TurnTracker.Server/localhost.key --cert ../TurnTracker.Server/localhost.crt -o",
    "update-build-date": "node src/environments/setBuildDate.js",
    "build-prod": "ng build --configuration production",
    "update-service-worker": "node src/updateServiceWorker.js",
    "build": "npm run-script update-build-date && npm run-script build-prod && npm run-script update-service-worker",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.16",
    "@angular/cdk": "^11.2.10",
    "@angular/common": "^12.2.16",
    "@angular/compiler": "^12.2.16",
    "@angular/core": "^12.2.16",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^12.2.16",
    "@angular/material": "^11.2.10",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@angular/router": "^12.2.16",
    "@angular/service-worker": "^12.2.16",
    "core-js": "^3.6.4",
    "encodr": "^1.2.2",
    "luxon": "^1.22.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.0.0",
    "websocket-extensions": "^0.1.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.18",
    "@angular/cli": "^12.2.18",
    "@angular/compiler-cli": "^12.2.16",
    "@angular/language-service": "^12.2.16",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/luxon": "^1.21.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "^8.6.2",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5",
    "webpack-subresource-integrity": "^1.5.2"
  }
}
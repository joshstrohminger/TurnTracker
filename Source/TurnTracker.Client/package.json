{
  "name": "turntracker-client",
  "version": "1.6.0",
  "description": "Turn Tracker",
  "scripts": {
    "ng": "node_modules/.bin/ng",
    "start": "node_modules/.bin/ng serve --open",
    "start-server": "http-server -p 8080 -c-1 ../TurnTracker.Server/wwwroot --ssl --key ../TurnTracker.Server/localhost.key --cert ../TurnTracker.Server/localhost.crt -o",
    "update-build-date": "node src/environments/setBuildDate.js",
    "build-prod": "node_modules/.bin/ng build --prod",
    "update-service-worker": "node src/updateServiceWorker.js",
    "build": "npm run-script update-build-date && npm run-script build-prod && npm run-script update-service-worker",
    "test": "node_modules/.bin/ng test",
    "lint": "node_modules/.bin/ng lint",
    "e2e": "node_modules/.bin/ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.2",
    "@angular/cdk": "^9.0.1",
    "@angular/common": "^9.0.2",
    "@angular/compiler": "^9.0.2",
    "@angular/core": "^9.0.2",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "^9.0.2",
    "@angular/material": "^9.0.1",
    "@angular/platform-browser": "^9.0.2",
    "@angular/platform-browser-dynamic": "^9.0.2",
    "@angular/router": "^9.0.2",
    "@angular/service-worker": "^9.0.2",
    "core-js": "^3.6.4",
    "encodr": "^1.2.2",
    "luxon": "^1.22.0",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "websocket-extensions": "^0.1.4",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.1",
    "@angular/cli": "^9.1.0",
    "@angular/compiler-cli": "^9.0.2",
    "@angular/language-service": "^9.0.2",
    "@types/jasmine": "^3.5.1",
    "@types/jasminewd2": "^2.0.8",
    "@types/luxon": "^1.21.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "protractor": "^5.4.0",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "typescript": "~3.7.5"
  }
}
